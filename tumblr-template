<script src="http://static.tumblr.com/q0etgkr/lPHlj76n8/infinitescrollingbycodysherman.js"></script>
{/block:IfInfiniteScrolling}


{block:PermalinkPage}{block:Posts}{block:Video}
<!-- FitVids -->
<script src="http://static.tumblr.com/6az34or/j3Wn29uox/fitvids.min.js"></script>
{/block:Video}{/block:Posts}{block:PermalinkPage}

<script>



	$(document).ready(function(){


	{block:PermalinkPage}{block:Posts}{block:Video}
	// fitvids
	$(".videoplayer").fitVids();
	{/block:Video}{/block:Posts}{block:PermalinkPage}


	// hide #back-top first
	$("#back-top").hide();

	// fade in #back-top
	$(function () {
		$(window).scroll(function () {
			if ($(this).scrollTop() > 100) {
				$('#back-top').fadeIn();
			} else {
				$('#back-top').fadeOut();
			}
		});

		// scroll body to 0px on click
		$('#back-top a').click(function () {
			$('body,html').animate({
				scrollTop: 0
			}, 800);
			return false;
		});
	});

}); //end document ready
</script>

