		{block:ifNotReversePagination}
		<!-- not reversed pagination -->
			{block:PreviousPost}<a href="{PreviousPost}" class="page-number previous l-left">&#8592; {lang:Previous}</a>{/block:PreviousPost}
			{block:NextPost}<a href="{NextPost}" class="page-number next l-right">{lang:Next} &#8594;</a>{/block:NextPost}
		{/block:ifNotReversePagination}

		{block:ifReversePagination}
		<!-- reversed pagination -->
			{block:NextPost}<a href="{NextPost}" class="page-number next l-left">&#8592; {lang:Previous}</a>{/block:NextPost}
			{block:PreviousPost}<a href="{PreviousPost}" class="page-number previous l-right">{lang:Next} &#8594;</a>{/block:PreviousPost}
		{/block:ifReversePagination}

	</div>
